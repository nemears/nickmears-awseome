
// XTERM
XTerm*background: rgb:1/51/51
XTerm*foreground: salmon
XTerm*faceName: DejaVu Sans Mono:size=12:antialias=false
XTerm*translations: #override \
	Shift Ctrl <Key> C: copy-selection(CLIPBOARD) \n\
	Shift Ctrl <Key> V: insert-selection(CLIPBOARD)

// URXVT
URxvt*background: rgb:1/51/51
URxvt*foreground: salmon
URxvt*font: xft:DejaVuSansMono:pixelsize=18
URxvt*scrollstyle: plain
URxvt*scrollColor: #addbdb
URxvt*scrollBar_right: true
URxvt.keysym.Shift-Control-C: eval:copy_clipboard
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt*depth: 32

URxvt*color0: #FA8072
URxvt*color1: #ffafa6
URxvt*color2: #ffd6d1
URxvt*color3: #ffefed
URxvt*color4: #ffffff
URxvt*color5: #f0fffe
URxvt*color6: #e0fffc
URxvt*color7: #c7fffa
URxvt*color8: #a8fff7
URxvt*color9: #91fff5
URxvt*color10: #73fff2
URxvt*color11: #54fff0
URxvt*color12: #38fff8
URxvt*color13: #38e8ff
URxvt*color14: #0df7ff
URxvt*color15: #0de3ff
